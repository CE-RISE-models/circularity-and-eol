<https://ce-rise-models.codeberg.page/circularity-and-eol/.owl.ttl> a owl:Ontology ;
    rdfs:label "circularity-and-eol" ;
    dcterms:license "CC BY-NC 4.0" ;
    dcterms:title "CE-RISE Circularity and End-of-Life Data Model" ;
    pav:version "0.0.1" ;
    skos:definition """Data model capturing circularity and end-of-life information for products, including design-for-circularity attributes, performance factors and scores, and end-of-life pathways such as disassembly, recycling, treatment options, and CRM recovery-related information.
""" .

circ:CircularityAndEOL a owl:Class ;
    rdfs:label "CircularityAndEOL" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty circ:circularity_assessments ],
        [ a owl:Restriction ;
            owl:allValuesFrom circ:DesignForCircularity ;
            owl:onProperty circ:design_for_circularity ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty circ:design_for_circularity ],
        [ a owl:Restriction ;
            owl:allValuesFrom circ:EndOfLifeInformation ;
            owl:onProperty circ:end_of_life_information ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty circ:circularity_assessments ],
        [ a owl:Restriction ;
            owl:allValuesFrom circ:CircularityAssessments ;
            owl:onProperty circ:circularity_assessments ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty circ:product_performance_factors ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty circ:end_of_life_information ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty circ:end_of_life_information ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty circ:design_for_circularity ],
        [ a owl:Restriction ;
            owl:allValuesFrom circ:ProductPerformanceFactors ;
            owl:onProperty circ:product_performance_factors ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty circ:product_performance_factors ] ;
    skos:definition "Root container for circularity and end-of-life information" ;
    skos:inScheme circ: ;
    circ:references "ESPR; CIRPASS; Battery Pass; King et al.; Jensen et al.; Berger et al.; Koppelaar et al.; Cruz et al.; Christensen et al." .

