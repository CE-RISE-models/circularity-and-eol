@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

circ:CircularityAndEOL a sh:NodeShape ;
    sh:closed true ;
    sh:description "Root container for circularity and end-of-life information" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class circ:DesignForCircularity ;
            sh:description "Design features that enable circular economy principles" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 0 ;
            sh:path circ:design_for_circularity ],
        [ sh:class circ:ProductPerformanceFactors ;
            sh:description "Quantifiable factors influencing circular performance" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 1 ;
            sh:path circ:product_performance_factors ],
        [ sh:class circ:CircularityAssessments ;
            sh:description "Generic container for circularity assessments" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 2 ;
            sh:path circ:circularity_assessments ],
        [ sh:class circ:EndOfLifeInformation ;
            sh:description "End-of-life pathways and recovery information" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 3 ;
            sh:path circ:end_of_life_information ] ;
    sh:targetClass circ:CircularityAndEOL .

